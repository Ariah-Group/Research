				<xs:element name="DefaultMailingAddress">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="work"/>
							<xs:enumeration value="home"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>			
				<xs:element name="InstitutionalAffiliation">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="IsAffiliated" type="globLib:YesNoDataType"/>
							<xs:element name="AffiliatedInstitution" type="FellowshipsSupplemental:FellowshipsSupplemental_AffiliatedInstitution_DataType" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>							
				<xs:element name="ScholarlyStatus">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Senior Scholar"/>
							<xs:enumeration value="Junior Scholar"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Reference1" type="FellowshipsSupplemental:FellowshipsSupplemental_Reference_DataType" minOccurs="0"/>
				<xs:element name="Reference2" type="FellowshipsSupplemental:FellowshipsSupplemental_Reference_DataType" minOccurs="0"/>
				<xs:element name="NominatingOfficial">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="IsNomExempt" type="globLib:YesNoDataType"/>
							<xs:element name="Official" type="FellowshipsSupplemental:FellowshipsSupplemental_Official_DataType" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>	
			</xs:sequence>
			<xs:attribute name="FormVersion" type="globLib:FormVersionDataType" use="required" fixed="1.0"/>
		</xs:complexType>
	</xs:element>
